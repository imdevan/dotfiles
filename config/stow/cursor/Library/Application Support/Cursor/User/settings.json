    },
    "terminal.background": "#00000000"
  },
  "autoHidePlus.autoHideLeftSideBar": true,
  "autoHidePlus.autoHidePanel": true,
  "autoHidePlus.autoHideRightSideBar": true,
  "workbench.settings.applyToAllProfiles": [],
  "colorize.languages": [
    "javascript",
    "typescript",
    "markdown"
  ],
  "python.createEnvironment.trigger": "off",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "git.openRepositoryInParentFolders": "always",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "vscode_vibrancy.opacity": 0.025,
  "window.titleBarStyle": "custom",
  "vscode_vibrancy.refreshInterval": 500,
  "vscode_vibrancy.theme": "Catppuccin Mocha",
  "vscode_vibrancy.preferedLightTheme": "Catppuccin Mocha",
  "vscode_vibrancy.preferedDarkTheme": "Catppuccin Mocha",
  "terminal.integrated.gpuAcceleration": "off",
  "window.systemColorTheme": "dark",
  "window.autoDetectColorScheme": false
}
